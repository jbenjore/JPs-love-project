<h1>My Checkins</h1>

<table>
  <tr>
    <th>Shop</th>
    <th>Address</th>
    <th>Date</th>

  </tr>

<% @shop_checkins.each do |shop_checkin| %>
  <tr>
    <td><%= link_to shop_checkin.shop.name, "/shops/#{shop_checkin.shop_id}"%></td>
    <td><%= shop_checkin.shop.address %></td>
    <td><%= shop_checkin.created_at %></td>
  </tr>
<% end %>
</table>

<br />

<%= 
#link_to 'New Shop checkin', new_shop_checkin_path 
%>
